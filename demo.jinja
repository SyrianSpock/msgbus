{% extends "CMakeLists.txt.jinja" %}

{% block additional_targets %}
add_executable(
    demo
    {% for s in source + target.demo -%}
    {{ s }}
    {% endfor %}
    )

target_link_libraries(
    demo
    pthread
    )


add_executable(
    demo_pb
    {% for s in source + target.demo_protobuf -%}
    {{ s }}
    {% endfor %}
    )

target_link_libraries(
    demo
    pthread
    )

{% endblock %}
